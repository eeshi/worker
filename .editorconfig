# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]

# Two spaces indentation style
indent_style = space
indent_size = 2

# Unix-style newlines with a newline ending and utf-8 encoding in every file
end_of_line = lf
insert_final_newline = true
charset = utf-8

trim_trailing_whitespace = true